{
    "copyFiles": [
        {
            "from": [
                "node_modules/@nareshr/remarkjs-themes/src/css/**/*"
            ],
            "to": "static/remark/css"
        },
        {
            "from": [
                "node_modules/@nareshr/remarkjs-themes/src/fonts/**/*"
            ],
            "to": "static/remark/fonts"
        },
        {
            "from": [
                "node_modules/@nareshr/remarkjs-themes/src/js/**/*"
            ],
            "to": "static/remark/js"
        },
        {
            "from": [
                "node_modules/@nareshr/remarkjs-themes/src/index.dynamic.template.html",
                "node_modules/@nareshr/remarkjs-themes/src/header.template.html",
                "node_modules/@nareshr/remarkjs-themes/src/footer.template.html"
            ],
            "to": "static"
        },
        {
            "from": [
                "node_modules/@nareshr/remarkjs-themes/build-slides.js"
            ],
            "to": "."
        }
    ],
    "copyFilesSettings": {
        "whenFileExists": "overwrite"
    }
}